<map:sitemap xmlns:map="http://www.nexista.org/sitemap">

    <map:gate name="x--dev--flow.css" cache="300" content_type="text/css">
        <map:action type="header" params="Cache-Control,max-age=30"/>
        <map:action type="header" params="Content-type,text/css"/>
        <map:raw src="s/css/flow.css"/>
        <map:raw src="s/css/exception.css"/>
    </map:gate>

    <map:gate name="x--dev--flow.js" cache="300" content_type="application/javascript">
        <map:action type="header" params="Content-type,application/javascript"/>
        <map:raw src="s/js/flow.js"/>
    </map:gate>

    <map:gate name="x--dev--timex.js" cache="300" content_type="text/javascript">
        <map:action type="header" params="Content-type,text/javascript"/>
        <map:raw src="s/js/timer.js"/>
    </map:gate>

		
    <map:gate name="x--dev--jquery.js" cache="300" content_type="text/javascript">
        <map:action type="header" params="Content-type,application/javascript"/>
        <map:raw src="s/js/jquery/jquery-1.3.2.min.js"/>
    </map:gate>



		
    <map:gate name="x--dev--jquery.treeview.js" cache="300" content_type="text/javascript">
        <map:action type="header" params="Cache-Control,max-age=30"/>
        <map:action type="header" params="Content-type,text/javascript"/>
        <map:raw src="s/js/jquery/plugins/jquery.treeview.js"/>
    </map:gate>
    <map:gate name="x--dev--jquery.treeview.css" cache="300" content_type="text/css">
        <map:action type="header" params="Cache-Control,max-age=30"/>
        <map:action type="header" params="Content-type,text/css"/>
        <map:raw src="s/css/jquery.treeview.css"/>
    </map:gate>

</map:sitemap>
