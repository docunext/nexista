    <map:gate name="x--dev--jquery.xslTransform.js" cache="300" content_type="text/javascript">
        <map:action type="header" params="Content-type,text/javascript"/>
        <map:raw src="s/js/jquery/plugins/jquery.xslTransform.js"/>
